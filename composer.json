{
  "name": "silverware/gallery",
  "type": "silverstripe-vendormodule",
  "description": "SilverWare Gallery Module.",
  "homepage": "https://github.com/praxisnetau/silverware-gallery",
  "keywords": [
    "silverware",
    "gallery",
    "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "https://www.praxis.net.au"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverware/lightbox": "^1.0",
    "silverware/masonry": "^1.2",
    "silverware/silverware": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "SilverWare\\Gallery\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "admin/client/dist",
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
